{
  "name": "Magnetis Summary",
  "short_name": "MagSummary",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "notifications"],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": [
    {
      "src": "images/icons/logo-48.jpg",
      "sizes": "48x48"
    },
    {
      "src": "images/icons/logo-72.jpg",
      "sizes": "72x72"
    },
    {
      "src": "images/icons/logo-144.jpg",
      "sizes": "144x144"
    },
    {
      "src": "images/icons/logo-192.jpg",
      "sizes": "192x192"
    },
    {
      "src": "images/icons/logo-192.png",
      "sizes": "192x192"
    },
    {
      "src": "images/icons/logo-512.png",
      "sizes": "512x512"
    }
  ],
  "start_url": "./index.html",
  "display": "standalone",
  "theme_color": "#22292f",
  "background_color": "#22292f"
}
